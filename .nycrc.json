{
  "all": true,
  "extension": [
    ".ts"
  ],
  "include": [
    "src/utils/**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts",
    "tests/**",
    "dist/**",
    "examples/**",
    "src/testing/**"
  ],
  "reporter": [
    "text-summary",
    "lcov"
  ],
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}